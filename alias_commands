# ALIAS COMMANDS

# docker
alias dk="docker"

# cd
alias hh="cd ~/"

# git 
alias cl="git clone"
alias st="git status"
alias br="git branch -v"
alias ck="git checkout"
alias br="git branch -v"
alias am="git commit -am"
alias cm="git commit -m"
alias gr="git remote -v"
alias ga="git add"
alias lg="git log"
alias dff="git diff"
alias psh="git push"
alias pll="git pull"

# system
alias ee="exit"
alias la="ls -la"
alias c="clear"

alias storm="docker run --rm -itd -v /home/kasm-user/Desktop/storm/oprs:/root/.openosrs -v /tmp/.X11-unix:/tmp/.X11-unix:ro -e DISPLAY=:1.0 dcat23/storm:gui"
