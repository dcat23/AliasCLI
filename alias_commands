# ALIAS COMMANDS

# docker
alias dk="docker"

# cd
alias hh="cd ~/"

# git 
alias cl="git clone"
alias st="git status"
alias br="git branch -v"
alias ck="git checkout"
alias br="git branch -v"
alias am="git commit -am"
alias cm="git commit -m"
alias gr="git remote -v"
alias ga="git add"
alias lg="git log"
alias dff="git diff"
alias psh="git push"
alias pll="git pull"

# system
alias ee="logout"
alias la="ls -la"

alias storm="~/java/bin/java -Xmx512m -Xss2m -XX:+DisableAttachMechanism -jar ~/Storm-Launcher-*.jar"

alias xstop="sudo systemctl stop xrdp"
alias xstar="sudo systemctl start xrdp"
alias mt="mate-terminal"
